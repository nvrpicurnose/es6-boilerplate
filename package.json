{
  "name": "es6-boilerplate",
  "version": "0.0.0",
  "description": "My basic boilerplate for setting up ES6 projects for node.js.",
  "main": "dist/index.js",
  "scripts": {
    "build": "gulp build",
    "dev": "npm run build && npm run watch",
    "start": "npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node dist/index.js",
    "watch": "nodemon --watch dist dist/index.js & gulp watch"
  },
  "author": "Leon Li",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-watch": "^4.3.8",
    "nodemon": "^1.9.2"
  },
  "dependencies": {
    "lodash": "^4.13.1"
  }
}
